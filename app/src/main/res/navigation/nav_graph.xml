<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/login_fragment">

    <fragment
        android:id="@+id/login_fragment"
        android:name="me.ayra.ha.healthconnect.ui.LoginFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_login" />

    <fragment
        android:id="@+id/home_fragment"
        android:name="me.ayra.ha.healthconnect.ui.HomeFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/stats_fragment"
        android:name="me.ayra.ha.healthconnect.ui.StatsFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_stats" />

    <fragment
        android:id="@+id/settings_fragment"
        android:name="me.ayra.ha.healthconnect.ui.SettingsFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_settings" />

    <fragment
        android:id="@+id/settings_health_data_fragment"
        android:name="me.ayra.ha.healthconnect.ui.HealthDataPreferenceFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_settings" />
</navigation>